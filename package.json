{
  "name": "burryport-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "import:osm": "cross-env DOTENV_CONFIG_PATH=.env.osm node -r dotenv/config scripts/import_osm.js",
    "import:fhrs": "cross-env DOTENV_CONFIG_PATH=.env.fhrs node -r dotenv/config scripts/import_fhrs.js",
    "new:day": "powershell -NoProfile -ExecutionPolicy Bypass -File scripts/new-day.ps1",
    "dev": "vite",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "build": "tsc -p tsconfig.json --noEmit && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "prebuild": "node scripts/generate-sitemap.mjs"
    
  },
  "dependencies": {
    "@mdx-js/mdx": "^3.1.1",
    "@mdx-js/react": "^3.1.1",
    "@supabase/supabase-js": "^2.56.1",
    "axios": "^1.11.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@mdx-js/rollup": "^3.1.1",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.0",
    "cross-env": "^10.0.0",
    "dotenv": "^17.2.1",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "supabase": "^2.40.7",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2"
  }
}
